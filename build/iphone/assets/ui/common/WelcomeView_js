function WelcomeView(){var e=Ti.UI.createView({backgroundImage:"images/WelcomePage/welcomeBG.png"}),o=Ti.UI.createButton({backgroundImage:"images/WelcomePage/joinBtn_welcomePG.png",width:pxToDP(421),height:pxToDP(78),top:320});return o.addEventListener("click",function(){var o=require("ui/handheld/LoginWindow"),n=new o;e.animate(get_darken),setTimeout(function(){n.open(get_lighter)},600)}),e.add(o),e}Ti.include("../logic/Animations.js"),Ti.include("../logic/GraphicFunctions.js"),module.exports=WelcomeView;